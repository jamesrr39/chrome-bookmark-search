{
	"name": "chrome-bookmark-search-extension",
	"description": "bookmark search extension for google chrome",
	"version": "0.0.1",
	"private": true,
	"main": "index.js",
	"repository": "https://github.com/jamesrr39/chrome-bookmark-search.git",
	"license": "GPL-2.0-only",
	"packageManager": "yarn@4.7.0",
	"devDependencies": {
		"@types/chrome": "^0.0.308",
		"@types/lunr": "^2",
		"typescript": "^5.8.2"
	},
	"dependenciesMeta": {
		"@types/chrome@0.0.308": {
			"unplugged": true
		},
		"preact@10.26.4": {
			"unplugged": true
		},
		"preact-jsx-runtime@1.2.0": {
			"unplugged": true
		}
	},
	"dependencies": {
		"esbuild": "^0.25.1",
		"lunr": "^2.3.9",
		"preact": "^10.26.4",
		"preact-jsx-runtime": "^1.2.0"
	},
	"scripts": {
		"clean": "rm -rf dist/*",
		"copy-non-js-assets": "cp src/index.html dist/ && cp src/manifest.json dist/ && cp -r src/icons dist/ && cp -r src/bootstrap dist/",
		"esbuild": "esbuild src/main.tsx --bundle --sourcemap --outfile=dist/main.js",
		"config-mock": "rm -f src/dal/bookmarkFetcher.ts && ln -s mockBookmarkFetcher.ts src/dal/bookmarkFetcher.ts",
		"config-prod": "rm -f src/dal/bookmarkFetcher.ts && ln -s chromeBookmarkFetcher.ts src/dal/bookmarkFetcher.ts",
		"dev": "yarn clean && yarn copy-non-js-assets && yarn config-mock && yarn esbuild --watch",
		"build": "yarn clean && yarn copy-non-js-assets && yarn config-prod && yarn esbuild"
	}
}
