{
	"manifest_version": 2,
	"name": "Bookmark Search",
	"description": "Intelligently search through your chrome bookmarks",
	"version": "0.0.9",
	"minimum_chrome_version": "38",
	"browser_action": {
		"default_icon": {
			"16": "icons/my-view-16.png"
		},
		"default_title": "Bookmark Search",
		"default_popup": "index.html"
	},
	"permissions": [
		"tabs",
		"bookmarks",
		"chrome://favicon/"
	]
}
